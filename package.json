{
  "name": "StudyAtHome",
  "version": "1.0.0",
  "description": "Ouchi De Jikanwari Web Version",
  "author": "Nekoya3",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon src/server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node src/server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "clean": "rimraf .nuxt dist",
    "copy": "yarn copy:client && yarn copy:server",
    "copy:client": "cpx -C \"src/static/**\" dist/client/ && cpx -C \".nuxt/dist/**\" dist/client/assets/",
    "copy:server": "cpx -C \"functions/**\" dist/server && cpx \".nuxt/**\" dist/server/",
    "install:functions": "yarn install --cwd dist/server",
    "firebase-emulator": "yarn clean && yarn lint && yarn build && yarn copy && yarn install:functions && firebase emulators:start"
  },
  "lint-staged": {
    "*.{js,vue}": "yarn lint",
    "*.{css,scss,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.9.6",
    "@nuxt/typescript-runtime": "^0.4.6",
    "@nuxtjs/axios": "^5.10.3",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/pwa": "^3.0.0-0",
    "@types/js-cookie": "^2.2.6",
    "cookieparser": "^0.1.0",
    "core-js": "3.6.5",
    "cross-env": "^7.0.2",
    "dayjs": "^1.8.26",
    "express": "^4.17.1",
    "firebase": "^7.14.3",
    "firebase-admin": "^8.12.0",
    "js-cookie": "^2.2.1",
    "jwt-decode": "^2.2.0",
    "lodash.clonedeep": "^4.5.0",
    "nuxt": "^2.12.2",
    "nuxt-property-decorator": "^2.7.2",
    "nuxt-svg-loader": "^1.2.0",
    "nuxt-webfontloader": "^1.1.0",
    "uuid": "^8.0.0",
    "vuex-class-component": "^2.3.2",
    "vuex-persistedstate": "^3.0.1"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.6.6",
    "@nuxtjs/eslint-config": "^2.0.2",
    "@nuxtjs/eslint-config-typescript": "^2.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/stylelint-module": "^3.2.2",
    "@nuxtjs/vuetify": "^1.11.2",
    "babel-eslint": "^10.1.0",
    "cpx": "^1.5.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-tsdoc": "^0.2.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "nodemon": "^2.0.3",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.3.3",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.17.2"
  }
}
